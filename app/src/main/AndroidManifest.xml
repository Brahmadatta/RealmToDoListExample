<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="Example.com.realmtodolistexample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="Example.com.realmtodolistexample.TaskApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="Example.com.realmtodolistexample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Example.com.realmtodolistexample.MovieRecyclerViewActivity" />
        <activity android:name="Example.com.realmtodolistexample.OfflineDataActivity"></activity>
    </application>

</manifest>